{
  "name": "kmap-solver-lib",
  "version": "0.0.3",
  "description": "A utility function to solve karnaugh maps up to 4 variables",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npx ava",
    "inspect": "node --inspect -r ts-node/register ./src/index.tsx",
    "build": "tsc -b",
    "watch": "tsc -b --watch"
  },
  "keywords": [
    "karnaugh",
    "maps",
    "utility",
    "typescript",
    "logic",
    "boolean",
    "algebra"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obsfx/kmap-solver-lib.git"
  },
  "author": "Ömercan Balandı",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/obsfx/kmap-solver-lib/issues"
  },
  "homepage": "https://github.com/obsfx/kmap-solver-lib#readme",
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/node": "^14.14.10",
    "ava": "^3.14.0",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
